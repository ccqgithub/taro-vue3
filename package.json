{
	"name": "taro-vue3",
	"version": "1.0.0",
	"private": true,
	"description": "",
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "less"
	},
	"scripts": {
		"build:weapp-test": "cross-env TARO_RELEASE=false taro build --type miniprogram",
		"build:weapp": "cross-env TARO_RELEASE=true taro build --type miniprogram",
		"dev:weapp": "taro build --type miniprogram --watch",
		"build:h5": "taro build --type h5",
		"dev:h5": "cross-env TARO_H5_ENV=dev yarn run build:h5 -- --watch",
		"style:dts": "node scripts/cssModulesDTS.js",
		"lint": "npm-run-all --parallel lint:*",
		"lint:js": "eslint . --ext .js --ext .ts",
		"lint:css": "stylelint '**/*.{css,less,scss}'",
		"lint:ts": "vue-tsc --noEmit --skipLibCheck -p tsconfig.json",
		"prepare": "husky install"
	},
	"author": "",
	"dependencies": {
		"@babel/runtime": "7.17.9",
		"@tarojs/components": "3.4.8",
