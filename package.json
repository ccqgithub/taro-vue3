		"@types/webpack-env": "1.16.4",
		"@typescript-eslint/eslint-plugin": "5.20.0",
		"@typescript-eslint/parser": "5.20.0",
		"@vue/compiler-sfc": "3.2.33",
		"babel-preset-taro": "3.4.8",
		"cross-env": "7.0.3",
